  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  width: 44px;
  height: 44px;
  background: #e5d2f2;
  border: 1px solid #ffffff;
  color: #9747ff;
  box-sizing: border-box;
}

.quiz_num span {
  -webkit-transition: 1s all;
  transition: 1s all;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 34px;
  width: 34px;
  background: #fff;
  border-radius: 50%;
