}

.quiz_form form input.active:valid,
.quiz_form form input.active:-webkit-autofill:valid,
.quiz_form form input.active:-webkit-autofill:hover:valid,
.quiz_form form input.active:-webkit-autofill:focus:valid,
.quiz_form form input.active:-webkit-autofill:active:valid {
  border: 1px solid #68c853;
  background-color: #fff !important;
  background-repeat: no-repeat;
  background-position: top 50% right 16px;
}

.quiz_form form button {
  text-transform: uppercase;
  margin-top: 11px;
  border-radius: 24px;
  height: 40px;
  display: block;
  text-align: center;
  width: 100%;
  background: #f64777;
  font-style: normal;
  font-weight: 700;
  font-size: 16px;
  line-height: 40px;
  letter-spacing: 0.03em;
  border: none;
  color: #ffffff;
  cursor: pointer;
  transition: all 0.3s ease;
}

.quiz_form form button:hover {
  background: #ff5a8a;
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(246, 71, 119, 0.4);
}

.quiz_form form button:focus {
  outline: none;
  background: #ff5a8a;
  box-shadow: 0 0 0 3px rgba(246, 71, 119, 0.3);
}

.quiz_form form button:active {
  background: #d93a60;
  transform: translateY(0);
  box-shadow: 0 2px 6px rgba(246, 71, 119, 0.3);
}

.quiz_form form button:disabled {
  background: #cccccc;
  cursor: not-allowed;
  transform: none;
  box-shadow: none;
}

